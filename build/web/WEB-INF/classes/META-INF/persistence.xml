<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="CreditPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.credit.dao.CreditUtilidad</class>
    <class>com.credit.dao.Empresa</class>
    <class>com.credit.dao.Rol</class>
    <class>com.credit.dao.CreditPagocapital</class>
    <class>com.credit.dao.CreditElectrodomestico</class>
    <class>com.credit.dao.Puesto</class>
    <class>com.credit.dao.CreditTiposolicitante</class>
    <class>com.credit.dao.CreditPagointeres</class>
    <class>com.credit.dao.CreditSolicitud</class>
    <class>com.credit.dao.Asignarprivilegio</class>
    <class>com.credit.dao.CreditConyugue</class>
    <class>com.credit.dao.CreditCliente</class>
    <class>com.credit.dao.Departamento</class>
    <class>com.credit.dao.CreditIngreso</class>
    <class>com.credit.dao.CreditFinanciamiento</class>
    <class>com.credit.dao.Asignarrol</class>
    <class>com.credit.dao.CreditRequisito</class>
    <class>com.credit.dao.Privilegio</class>
    <class>com.credit.dao.CreditTipocredito</class>
    <class>com.credit.dao.CreditAsociado</class>
    <class>com.credit.dao.Colaborador</class>
    <class>com.credit.dao.CreditTipocuota</class>
    <class>com.credit.dao.CreditCotizacion</class>
    <class>com.credit.dao.CreditPersona</class>
    <class>com.credit.dao.Agencia</class>
    <class>com.credit.dao.CreditDestinofondo</class>
    <class>com.credit.dao.CreditExclusivo</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/ApplicationManager?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
